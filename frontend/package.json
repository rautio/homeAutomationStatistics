{
  "name": "home-automation-statistics-frontend",
  "version": "1.0.0",
  "description": "Frontend for the homeAutomationStatistics application.",
  "main": "./src/index.js",
  "scripts": {
    "test": "mocha --compilers js:babel-core/register --colors ./test/*.spec.js",
    "test:watch": "mocha --compilers js:babel-core/register --colors -w ./test/*.spec.js",
    "test:travis": "mocha --compilers js:babel-core/register ./test/*.spec.js",
    "cover": "babel-node node_modules/istanbul/lib/cli cover node_modules/mocha/bin/_mocha ./test/*.spec.js",
    "coveralls": "npm run cover -- --report lcovonly && cat ./coverage/lcov.info | coveralls",
    "lint": "esw app test",
    "lint:watch": "esw -w app test",
    "clean": "rimraf build",
    "build": "webpack -p --env build",
    "dev": "webpack-dev-server --hot --inline --config webpack.config.js --env dev",
    "start": "npm-run-all --parallel dev"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.17.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "babili-webpack-plugin": "^0.1.2",
    "coveralls": "^3.0.0",
    "es6-promise": "^4.2.4",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-react-app": "^2.0.1",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-react": "^7.7.0",
    "eslint-watch": "^3.1.3",
    "expect": "^22.1.0",
    "exports-loader": "^0.7.0",
    "imports-loader": "^0.7.1",
    "istanbul": "^0.4.5",
    "material-ui": "^1.0.0-beta.34",
    "mocha": "^5.0.0",
    "mocha-lcov-reporter": "^1.3.0",
    "npm-run-all": "^4.1.2",
    "opener": "^1.4.3",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "rimraf": "^2.6.2",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.11.1",
    "yargs": "^11.1.0"
  },
  "devDependencies": {
    "babel-preset-react-hmre": "^1.1.1",
    "css-loader": "^0.28.11",
    "node-sass": "^4.7.2",
    "sass-loader": "^6.0.6",
    "scss-loader": "0.0.1",
    "style-loader": "^0.20.3",
    "webpack-cli": "^2.0.14"
  }
}
